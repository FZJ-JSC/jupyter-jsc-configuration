    User userlabs
    HostName userlabs-mgr-svc-ssh.userlabs.svc.cluster.local
    Port 22
    IdentityFile /home/tunnel/keys/tunnel
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    ControlPath /home/tunnel/ssh_socket/tunnel_hdfcloud
    ControlMaster auto
    ControlPersist 0

