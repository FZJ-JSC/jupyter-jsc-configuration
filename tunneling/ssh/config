Host *
    ServerAliveInterval 60
    ServerAliveCountMax 5

# HDF-Cloud Kubernetes
Host available_hdfcloud
    User userlabs
    HostName userlabs-mgr-svc-ssh.userlabs.svc.cluster.local
    Port 22
    IdentityFile /home/tunnel/keys/available
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    ControlPath /home/tunnel/ssh_socket/available_hdfcloud
    ControlMaster auto
    ControlPersist 0

Host remote_hdfcloud
    User userlabs
    HostName userlabs-mgr-svc-ssh.userlabs.svc.cluster.local
    Port 22
    IdentityFile /home/tunnel/keys/remote
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    ControlPath /home/tunnel/ssh_socket/remote_hdfcloud
    ControlMaster auto
    ControlPersist 0

